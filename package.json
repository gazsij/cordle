{
  "name": "cordle",
  "version": "1.0.0",
  "description": "Discord bot Wordle clone",
  "main": "./build/index.js",
  "type": "commonjs",
  "engines": {
    "node": "^16.9.*",
    "yarn": "^1.22.*"
  },
  "scripts": {
    "lint": "eslint --fix --ext .ts,.tsx .",
    "prepare": "husky install",
    "clean": "npx rimraf ./yarn.lock && npx rimraf ./**/node_modules",
    "build": "tsc",
    "prestart": "npm run lint && npm run build",
    "start": "node .",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
  },
  "author": "gazsij",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@typegoose/typegoose": "^9.5.0",
    "canvas": "^2.9.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^14.3.2",
    "glob": "^7.2.0",
    "mongodb": "^4.3.1",
    "mongoose": "^6.1.8",
    "p-event": "^5.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/glob": "^7.2.0",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
